{
  "name": "MyWorkFlowy",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.0",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "build-css": "node-sass src/sass/ -o src/css/",
    "watch-css": "npm run build-css && node-sass src/sass/ -o src/css/ --watch",
    "start": "react-scripts start & npm run watch-css",
    "build": "react-scripts build && npm run build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
